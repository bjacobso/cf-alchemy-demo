{
  "name": "alchemy-do-demo",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "ln -sf ../../../libsodium/dist/modules-esm/libsodium.mjs node_modules/libsodium-wrappers/dist/modules-esm/libsodium.mjs 2>/dev/null || true",
    "build:css": "npx tsx scripts/build-css.ts",
    "dev": "npm run build:css && npx tsx alchemy.run.ts",
    "deploy": "npm run build:css && npx tsx alchemy.run.ts",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "typecheck": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "dependencies": {
    "@effect/platform": "^0.77",
    "alchemy": "latest",
    "dotenv": "^17.2.3",
    "effect": "^3.12",
    "libsodium": "^0.7.16"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/cli": "^4",
    "@types/node": "^25.0.3",
    "oxfmt": "^0.1",
    "oxlint": "^1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4",
    "typescript": "^5.9.3"
  }
}
